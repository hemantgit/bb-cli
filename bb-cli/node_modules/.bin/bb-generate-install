#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@bb-cli/bb-generate/bin/bb-generate-install.js" "$@"
  ret=$?
else 
  node  "$basedir/../@bb-cli/bb-generate/bin/bb-generate-install.js" "$@"
  ret=$?
fi
exit $ret
